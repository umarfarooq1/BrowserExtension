{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "permissions": ["tabs","storage","webRequest","webRequestBlocking","history","<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}