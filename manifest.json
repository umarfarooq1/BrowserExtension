{
  "manifest_version": 2,
  "name": "Web Usage Survey",
  "version": "0.1",
  "permissions": [ "tabs","storage","cookies","webRequest","webRequestBlocking","history","<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  // <meta http-equiv="Content-Security-Policy" content="default-src *; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval' http://platform.linkedin.com ">
// 
}