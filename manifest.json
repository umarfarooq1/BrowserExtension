{
  "manifest_version": 2,
  "name": "Web Usage Survey (Without History Collection)",
  "version": "0.2",
  "permissions": [ "tabs","storage","cookies","webRequest","webRequestBlocking","<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'" 
}